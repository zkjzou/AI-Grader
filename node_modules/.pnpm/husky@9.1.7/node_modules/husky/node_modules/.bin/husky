#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhanfengzheng/Desktop/AI-Grader/node_modules/.pnpm/husky@9.1.7/node_modules/husky/node_modules:/Users/zhanfengzheng/Desktop/AI-Grader/node_modules/.pnpm/husky@9.1.7/node_modules:/Users/zhanfengzheng/Desktop/AI-Grader/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhanfengzheng/Desktop/AI-Grader/node_modules/.pnpm/husky@9.1.7/node_modules/husky/node_modules:/Users/zhanfengzheng/Desktop/AI-Grader/node_modules/.pnpm/husky@9.1.7/node_modules:/Users/zhanfengzheng/Desktop/AI-Grader/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
